10 PROMPT$ = "ROCKETSH"
11 PRINT ""
12 PRINT "Welcome to ";
13 COLOR 14
14 PRINT "Retro Rocket ";
15 COLOR 7
16 PRINT "BASIC shell."
17 PRINT "Type END to exit the shell and shut down."
18 PRINT ""
20 COLOR 14
30 PRINT PROMPT$;
40 COLOR 7
50 PRINT "> ";
60 INPUT inp$
70 EVAL inp$
80 IF ERROR = 1 THEN PRINT "Execution error: " + ERROR$
90 GOTO 20
