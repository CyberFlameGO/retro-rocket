10 CLS
20 FOR X = 0 TO 10
30 PRINT ""
40 NEXT
50 THICKNESS = 30
20 GCOL RGB(255,0,0)
30 RECTANGLE 0,0,799,THICKNESS
40 RECTANGLE 0,599-THICKNESS,799-THICKNESS,599
50 RECTANGLE 0,0,THICKNESS,599
60 RECTANGLE 799-THICKNESS,0,799,599
70 GCOL RGB(0,255,0)
80 CIRCLE 400, 300, 195, TRUE
90 GCOL RGB(0,0,255)
100 CIRCLE 400, 300, 200, FALSE
110 GCOL RGB(255,255,0)
120 TRIANGLE 800/2,600/2-THICKNESS,800/2-THICKNESS,600/2+THICKNESS,800/2+THICKNESS,600/2+THICKNESS
130 END